{
  "name": "react-bare-app",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "lint": "eslint ./src rollup.config.js",
    "build": "rollup -c",
    "build:watch": "yarn build -- --watch",
    "test": "BABEL_ENV=test mocha -c --require babel-register --recursive ./src",
    "test:watch": "yarn test -- --watch",
    "sample-app:dev-server": "webpack-dev-server --progress --colors -d --config examples/sample-app/webpack.config.js --context examples/sample-app/ --content-base examples/sample-app/",
    "sample-app:backend": "node examples/sample-app/server.js",
    "precommit": "yarn lint && yarn build && yarn test"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "cors": "^2.8.3",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.0.0",
    "express": "^4.15.3",
    "mocha": "^3.3.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^4.0.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4"
  },
  "dependencies": {
    "prop-types": "^15.5.9",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "styled-components": "^2.0.0"
  }
}
